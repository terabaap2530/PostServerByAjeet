# Facebook Group URL (use full URL, e.g. https://www.facebook.com/groups/123456789)
GROUP_URL=

# Default locked group name
LOCK_NAME=My Locked Group

# Server port
PORT=3000

# Optional: set headless to false for debugging locally
HEADLESS=true
